{
  "name": "tiner",
  "version": "0.0.8",
  "description": "Static server on-the-go",
  "keywords": [
    "server",
    "static",
    "development"
  ],
  "url": "https://github.com/sevastos",
  "author": "sevastos <m@sevastos.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sevastos/tiner.git"
  },
  "bugs": {
    "url": "https://github.com/sevastos/tiner/issues"
  },
  "preferGlobal": true,
  "config": {
    "port": 3030
  },
  "engines": {
    "node": ">= 0.8.x"
  },
  "main": "index.js",
  "bin": {
    "tiner": "./bin/tiner"
  },
  "scripts": {
    "start": "node index.js",
    "test": "make test",
    "blanket": {
        "pattern": "lib",
        "data-cover-never": "node_modules"
    }
  },
  "devDependencies": {
    "jshint": "2.x.x",
    "mocha": "1.x.x",
    "should": "1.x.x",
    "supertest": "0.x.x",
    "coveralls": "2.x.x",
    "mocha-lcov-reporter": "0.x.x",
    "blanket": "1.x.x"
  },
  "jshintConfig": {
    "predef": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "chai"
    ],
    "node": true,
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "indent": 2,
    "latedef": true,
    "newcap": true,
    "quotmark": "single",
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
